{
  "name": "vituum-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-s build:* && run-s build:format:*",
    "build:vite": "vite build",
    "build:format:html": "js-beautify dist/**/*.html && node node-scripts/minify-html-code.mjs",
    "build:format:css": "postcss dist/assets/**/*.css --dir dist/assets/css --ext css",
    "preview": "vite preview",
    "lint": "run-s lint:*",
    "lint:html": "markuplint dist/**/*.html && html-validate dist/**/*.html",
    "lint:stylelint": "stylelint src/**/*.{css,scss,sass}",
    "lint:eslint": "eslint src/**/*.{js,vue}",
    "fix": "run-s fix:*",
    "fix:stylelint": "stylelint --fix src/**/*.{css,scss,sass}",
    "fix:eslint": "eslint --fix src/**/*.{js,vue}",
    "fix:prettier": "prettier --write src/**/*.{css,scss,sass,js,vue}",
    "image-min": "node node-scripts/image-min.mjs",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@fullhuman/postcss-purgecss": "^6.0.0",
    "@markuplint/nunjucks-parser": "^4.5.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vituum/vite-plugin-nunjucks": "^1.1.0",
    "autoprefixer": "^10.4.19",
    "cheerio": "^1.0.0-rc.12",
    "clean-css": "^5.3.3",
    "eslint": "^9.1.1",
    "eslint-plugin-vue": "^9.25.0",
    "globals": "^15.1.0",
    "html-validate": "^8.18.2",
    "husky": "^9.0.11",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-keep-folder": "^5.3.2",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "js-beautify": "^1.15.1",
    "markuplint": "^4.6.1",
    "minify-inline-json": "^1.1.11",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "prettier": "3.2.5",
    "reset-css": "^5.0.2",
    "sass": "^1.75.0",
    "stylelint": "^16.4.0",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "terser": "^5.31.0",
    "vite": "^5.2.0",
    "vituum": "^1.1.0",
    "vue": "^3.4.26"
  },
  "browserslist": [
    "last 2 version"
  ],
  "resolutions": {
    "strip-ansi": "6.0.0",
    "string-width": "4.2.3"
  }
}
